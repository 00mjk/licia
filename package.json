{
  "name": "eris",
  "version": "0.8.0",
  "description": "Eustia modules",
  "bin": {
    "eris": "./bin/eris.js"
  },
  "scripts": {
    "ci": "npm run lint && npm run update && npm test",
    "update": "eris update",
    "pack": "eris pack",
    "lint": "eslint $ [a-z]",
    "serve": "nws -p 3000 -d testHtml",
    "test": "npm run test:node && npm run test:browser",
    "demo": "eris test -ah",
    "cov": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "eustia": "eustia",
    "test:node": "eris test -as && mocha",
    "test:browser": "eris test -kas && karma start",
    "test:sauce": "eris test -kas --sauce && karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/eris.git"
  },
  "keywords": [
    "eustia"
  ],
  "author": "redhoodsu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liriliri/eris/issues"
  },
  "homepage": "https://github.com/liriliri/eris#readme",
  "devDependencies": {
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.3",
    "chai": "^4.1.2",
    "chalk": "^2.2.0",
    "eslint": "^4.5.0",
    "eustia": "^0.5.0",
    "eustia-component": "0.0.3",
    "glob": "^7.0.3",
    "handlebars": "^4.0.5",
    "karma": "^1.7.1",
    "karma-chai-plugins": "^0.9.0",
    "karma-coverage": "^1.1.1",
    "karma-jquery": "^0.2.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sauce-launcher": "^1.1.0",
    "mocha": "^4.0.1",
    "ncp": "^2.0.0",
    "nopt": "^4.0.1",
    "promises-aplus-tests": "^2.1.2",
    "rmdir": "^1.2.0",
    "sinon": "^4.0.1",
    "underscore": "^1.8.3",
    "uuid": "^3.0.1"
  }
}
